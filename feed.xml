---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Anton Panasenko</title>
 <link href="http://blog.apanasenko.me/feed.xml" rel="self"/>
 <link href="http://blog.apanasenko.me/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://blog.apanasenko.me/</id>
 <author>
   <name>Recursive</name>
   <email>apanasenko@me.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://blog.apanasenko.me{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://blog.apanasenko.me{{ post.id }}</id>
   <content type="html">{{ post.excerpt | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>