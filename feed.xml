---
layout: none
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Anton Panasenko</title>
  <link href="http://blog.apanasenko.me/feed.xml" rel="self"/>
  <link href="http://blog.apanasenko.me/feed.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://dieu.github.com/</id>
  <author>
    <name>Anton Panasenko</name>
  </author>

  {% for post in site.posts %}
  <entry>
    <title type="html">{{ post.title | xml_escape }}</title>
    <link href="http://blog.apanasenko.me{{ post.url }}"/>
    <id>http://dieu.github.com{{ post.url }}</id>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <content type="html">
		{{ post.content | xml_escape }}
    </content>
  </entry>
  {% endfor %}
</feed>
